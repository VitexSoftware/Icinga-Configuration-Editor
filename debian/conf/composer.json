{
    "name": "vitexsoftware/icinga-editor",
    "description": "Editor of Icinga Configuration files (debianized)",
    "version": "1.2.1.2",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "vitex",
            "email": "info@vitexsoftware.cz"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "vendor-dir": "/var/lib/icinga-editor"
    },
    "require": {
        "deb/ease-fluentpdo": "*",
        "deb/ease-bootstrap": "*",
        "league/commonmark": "^0.15.0",
        "pear/mail": ">=1.3.0",
        "pear/mail_mime": ">=1.10.0"
    },
    "autoload": {
        "psr-4": {
            "Icinga\\Editor\\": "../../share/icinga-editor/classes/",
            "Icinga\\Editor\\Engine\\": "../../share/icinga-editor/classes/engine/",
            "Icinga\\Editor\\UI\\": "../../share/icinga-editor/classes/UI",
            "Icinga\\Editor\\modules\\": "../../share/icinga-editor/modules"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "/usr/share/php/EaseCore",
            "options": {
                "symlink": true
            }
        },
    {
      "type": "path",
      "url": "/usr/share/php/EaseHtml/"
    },
    {
      "type": "path",
      "url": "/usr/share/php/EaseBricks/"
    },
    {
      "type": "path",
      "url": "/usr/share/php/EaseTWB/"
    },
    {
      "type": "path",
      "url": "/usr/share/php/EaseFluentPDO/"
    },
    {
      "type": "path",
      "url": "/usr/share/php/EaseTWBWidgets/"
    }

    ]

}
